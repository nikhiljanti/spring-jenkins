pipeline{
    agent any
    stages{
        stage("Init"){
            steps{
                echo 'Initiating the spring-jenkins project.'
            }
        }
        stage("Build"){
            steps{
                echo 'Building the spring-jenkins project.'
            }
        }
        stage("Deploy"){
            steps{
                echo 'Deploying the spring-jenkins project.'
            }
        }
    }
}
